{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://stock-harvest-api.up.railway.app/api/$1"
    },
    {
      "source": "/((?\!api).*)",
      "destination": "/index.html"
    }
  ]
}
EOF < /dev/null